<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.listfragmentpractice"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/tree"
        android:label="City Gates"
        android:theme="@style/CustomActionBarTheme" >
        <activity
            android:name="com.example.citygates.MainActivity"
            android:label="City Gates" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.citygates.FullScreen"
            android:label="City Gates" >
        </activity>
        <activity
            android:name="com.example.citygates.MenuList"
            android:label="City Gates" >
        </activity>
        <activity
            android:name="com.example.citygates.MyListFragment"
            android:label="City Gates" >
        </activity>
        <activity
            android:name="com.example.citygates.MySocialMediaFragment"
            android:label="City Gates" >
        </activity>
        <activity
            android:name="com.example.citygates.SocialMediaAction"
            android:label="City Gates" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.CALL_BUTTON" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.citygates.MonthEventsFragment"
            android:label="City Gates" >
        </activity>
        <activity
            android:name="com.example.citygates.MeetOurPastors"
            android:label="City Gates" >
        </activity>
    </application>

</manifest>